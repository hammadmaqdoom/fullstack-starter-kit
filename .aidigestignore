yarn-debug.log*
yarn-error.log*
lerna-debug.log*
pnpm-debug.log*

# Environment files (never read these)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
.env.docker

# Testing coverage
coverage/
.nyc_output/

# IDE files
.vscode/
.idea/
*.swp
